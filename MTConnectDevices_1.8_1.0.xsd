				<xs:annotation>
					<xs:documentation>
            Millimeters per revolution.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MILLIMETER/SECOND">
				<xs:annotation>
					<xs:documentation>
            Millimeters per second
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MILLIMETER/SECOND^2">
				<xs:annotation>
					<xs:documentation>
            Acceleration in millimeters per second squared
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NEWTON">
