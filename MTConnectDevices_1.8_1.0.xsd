				<xs:annotation>
					<xs:documentation>
            A measurement of energy.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="KILOGRAM">
				<xs:annotation>
					<xs:documentation>
            Kilograms
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LITER">
				<xs:annotation>
					<xs:documentation>
            Measurement of volume of a fluid
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LITER/SECOND">
				<xs:annotation>
					<xs:documentation>
            Liters per second
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MICRO_RADIAN">
				<xs:annotation>
					<xs:documentation>
            Measurement of Tilt
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MILLIMETER">
				<xs:annotation>
					<xs:documentation>
            Millimeters
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
