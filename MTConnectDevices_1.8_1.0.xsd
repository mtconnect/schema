					<xs:documentation>
            Measurement of electrical energy, equal to one Joule
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REVOLUTION/SECOND">
				<xs:annotation>
					<xs:documentation>
            Revolutions per second.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REVOLUTION/SECOND^2">
				<xs:annotation>
					<xs:documentation>
            Revolutions per second squared.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GRAM/CUBIC_METER">
				<xs:annotation>
					<xs:documentation>
            Gram per cubic meter.
          </xs:documentation>
