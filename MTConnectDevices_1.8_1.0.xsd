	<xs:simpleType name="CompositionEnumTypeExtType">
		<xs:annotation>
			<xs:documentation>
        Extended tyoe for The vocab for the type of composition
      </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-ln-z][a-z]*:[A-Z_0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CompositionEnumTypeEnum">
		<xs:annotation>
			<xs:documentation>
        The vocab for the type of composition
      </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ACTUATOR">
				<xs:annotation>
					<xs:documentation>
            A mechanism for moving or controlling a mechanical part of a piece
            of equipment. It takes energy usually provided by air, electric
            current, or liquid and converts the energy into some kind of motion.
            
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AMPLIFIER">
				<xs:annotation>
					<xs:documentation>
            An electronic component or circuit for amplifying power, electric
            current, or voltage.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BALLSCREW">
				<xs:annotation>
					<xs:documentation>
            A mechanical structure for transforming rotary motion into linear
            motion.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BELT">
				<xs:annotation>
					<xs:documentation>
            An endless flexible band used to transmit motion for a piece of
            equipment or to convey materials and objects.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BRAKE">
				<xs:annotation>
					<xs:documentation>
            A mechanism for slowing or stopping a moving object by the
            absorption or transfer of the energy of momentum, usually by means
            of friction, electrical force, or magnetic force.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHAIN">
				<xs:annotation>
					<xs:documentation>
            An interconnected series of objects that band together and are used
            to transmit motion for a piece of equipment or to convey materials
            and objects.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHOPPER">
				<xs:annotation>
					<xs:documentation>
            A mechanism used to break material into smaller pieces.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHUCK">
				<xs:annotation>
					<xs:documentation>
            A mechanism that holds a part, stock material, or any other item in
            place.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHUTE">
				<xs:annotation>
					<xs:documentation>
            An inclined channel for conveying material.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CIRCUIT_BREAKER">
				<xs:annotation>
					<xs:documentation>
            A mechanism for interrupting an electric circuit.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CLAMP">
				<xs:annotation>
					<xs:documentation>
            A mechanism used to strengthen, support, or fasten objects in place.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COMPRESSOR">
				<xs:annotation>
					<xs:documentation>
            A pump or other mechanism for reducing volume and increasing
            pressure of gases in order to condense the gases to drive
            pneumatically powered pieces of equipment.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DOOR">
				<xs:annotation>
					<xs:documentation>
            A mechanical mechanism or closure that can cover a physical access
            portal into a piece of equipment allowing or restricting access to
            other parts of the equipment.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DRAIN">
				<xs:annotation>
					<xs:documentation>
            A mechanism that allows material to flow for the purpose of drainage
            from, for example, a vessel or tank.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ENCODER">
				<xs:annotation>
					<xs:documentation>
            A mechanism used to measure rotary position.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EXPOSURE_UNIT">
				<xs:annotation>
					<xs:documentation>
            A mechanism for emitting a type of radiation
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EXTRUSION_UNIT">
				<xs:annotation>
					<xs:documentation>
            A mechanism for dispensing liquid or powered materials
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FAN">
				<xs:annotation>
					<xs:documentation>
            Any mechanism for producing a current of air.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FILTER">
				<xs:annotation>
					<xs:documentation>
            Any substance or structure through which liquids or gases are passed
            to remove suspended impurities or to recover solids.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GALVANOMOTOR">
				<xs:annotation>
					<xs:documentation>
            An electromechanical actuator that produces deflection of a beam of
            light or energy in response to electric current through its coil in
            a magnetic field.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GRIPPER">
				<xs:annotation>
					<xs:documentation>
            A mechanism that holds a part, stock material, or any other item in
            place.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HOPPER">
				<xs:annotation>
					<xs:documentation>
            A chamber or bin in which materials are stored temporarily,
            typically being filled through the top and dispensed through the
            bottom.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LINEAR_POSITION_FEEDBACK">
				<xs:annotation>
					<xs:documentation>
            A mechanism that measures linear motion or position.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MOTOR">
				<xs:annotation>
					<xs:documentation>
            A mechanism that converts electrical, pneumatic, or hydraulic energy
            into mechanical energy.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OIL">
				<xs:annotation>
					<xs:documentation>
            A viscous liquid.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="POWER_SUPPLY">
				<xs:annotation>
					<xs:documentation>
            A unit that provides power to electric mechanisms.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PULLEY">
				<xs:annotation>
					<xs:documentation>
            A mechanism or wheel that turns in a frame or block and serves to
            change the direction of or to transmit force.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PUMP">
				<xs:annotation>
					<xs:documentation>
            An apparatus raising, driving, exhausting, or compressing fluids or
            gases by means of a piston, plunger, or set of rotating vanes.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REEL">
				<xs:annotation>
					<xs:documentation>
            A rotary storage unit for material
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SENSING_ELEMENT">
				<xs:annotation>
					<xs:documentation>
            A mechanism that provides a signal or measured value.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SPREADER">
				<xs:annotation>
					<xs:documentation>
            A mechanism for flattening or spreading materials
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STORAGE_BATTERY">
				<xs:annotation>
					<xs:documentation>
            A component consisting of one or more cells, in which chemical
            energy is converted into electricity and used as a source of power. 
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SWITCH">
				<xs:annotation>
					<xs:documentation>
            A mechanism for turning on or off an electric current or for making
            or breaking a circuit.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TABLE">
				<xs:annotation>
					<xs:documentation>
            A surface for holding an object or material
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TANK">
				<xs:annotation>
					<xs:documentation>
            A receptacle or container for holding material.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TENSIONER">
				<xs:annotation>
					<xs:documentation>
            A mechanism that provides or applies a stretch or strain to another
            mechanism.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TRANSFORMER">
				<xs:annotation>
