			<xs:enumeration value="AMPERE">
				<xs:annotation>
					<xs:documentation>
            Amps
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CELSIUS">
				<xs:annotation>
					<xs:documentation>
            Degrees Celsius
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COUNT">
				<xs:annotation>
					<xs:documentation>
            A count of something.
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DECIBEL">
				<xs:annotation>
					<xs:documentation>
            Sound Level
          </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DEGREE">
				<xs:annotation>
					<xs:documentation>
