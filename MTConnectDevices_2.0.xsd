    <xs:sequence>
      <xs:choice minOccurs='0' maxOccurs='1'>
        <xs:sequence minOccurs='1'>
          <xs:element name='Value' type='DataItemValueElementType' minOccurs='1' maxOccurs='unbounded'>
            <xs:annotation>
              <xs:documentation>
                single data value that is expected to be reported for a
                {{block(DataItem)}}. {{property(Value)}} **MUST NOT** be used in
                conjunction with any other {{block(Constraint)}} elements.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:sequence minOccurs='1'>
          <xs:element name='Minimum' type='DataItemNumericValueType' minOccurs='0' maxOccurs='1'>
            <xs:annotation>
              <xs:documentation>
                numeric lower constraint.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name='Maximum' type='DataItemNumericValueType' minOccurs='0' maxOccurs='1'>
