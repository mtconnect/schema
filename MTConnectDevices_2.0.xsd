  <xs:simpleType name='DataItemRelationshipTypeEnumType'>
    <xs:annotation>
      <xs:documentation>
        How the data items are related
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:string'>
      <xs:enumeration value='ATTACHMENT'>
        <xs:annotation>
          <xs:documentation>
            A reference to a DataItem that associates the values with an
            external entity
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='COORDINATE_SYSTEM'>
        <xs:annotation>
          <xs:documentation>
            The referenced DataItem provides the id of the effective Coordinate
            System
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='LIMIT'>
        <xs:annotation>
          <xs:documentation>
            The referenced DataItem provides process limits.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='OBSERVATION'>
