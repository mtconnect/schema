  </xs:simpleType>
  <xs:simpleType name='ReferenceIdType'>
    <xs:annotation>
      <xs:documentation>
        The item&apos;s reference to the data item or specificatiton
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:IDREF'/>
  </xs:simpleType>
  <xs:simpleType name='ComponentNameType'>
    <xs:annotation>
      <xs:documentation>
        The name of a related component
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:NMTOKEN'/>
  </xs:simpleType>
  <xs:simpleType name='CoordinateSystemIdRefType'>
    <xs:annotation>
      <xs:documentation>
        The id reference for the coordinate system associated with this data
        item
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:IDREF'/>
